<Application x:Class="test202.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:test202"
             xmlns:viewModel="clr-namespace:test202.MVVM.ViewModel"
             xmlns:view="clr-namespace:test202.MVVM.View">
             
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModel:HomeVM}">
            <view:Home/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewModel:DetailVM}">
            <view:Detail/>
        </DataTemplate>
    </Application.Resources>
</Application>
